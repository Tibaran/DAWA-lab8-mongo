<h2>Usuarios</h2>
<a type="button" class="btn btn-primary mb-3" href="/users/add">Agregar</a>
<table class="table table-striped table-inverse table-responsive">
    <thead class="thead-inverse">
        <tr>
            <th>Id</th>
            <th>Rol</th>
            <th>Nombre</th>
            <th>Correo</th>
            <th>Contrase√±a</th>
            <th>Imagen</th>
            <th>Estado</th>
            <th>Google</th>
        </tr>
        </thead>
        <tbody>
            {{#each user}}
            <tr>
                <td scope="row">{{_id}}</td>
                <td>{{rol}}</td>
                <td>{{nombre}}</td>
                <td>{{correo}}</td>
                <td>{{password}}</td>
                <td>{{img}}</td>
                <td>{{estado}}</td>
                <td>{{google}}</td>
            </tr>
            {{/each}}
        </tbody>
</table>